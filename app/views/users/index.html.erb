<div class="container">
  <div class="row">
<%= render 'users/list', user: @user, book: @book %>

<div class="col-md-8 offset-md-1">
<h1>Users</h1>
<table class="table table-striped">
  <thead>
  <tr>
    <th>image</th>
    <th>name</th>

<% @users.each do |user| %>
<table class="table table-striped">
  <th><%= image_tag user.get_profile_image(100,100) %></th>
  <th><%= user.name %></th>
<th><%= link_to "Show",  user_path(user.id) %></th>
<% end %>
</tr>
  </thead>
</table>
</div>
</div>
</div>

